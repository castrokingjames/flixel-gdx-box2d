<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.1//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.5.1/distro-source/core/src/gwt-module.dtd">
<module rename-to="org.flxbox2d.flxbox2d_examples_html5">	
	<inherits name="org.flxbox2d.flxbox2d_core" />
	<inherits name="org.flixel.flixel_html5" />
	<inherits name="org.flxbox2d.flxbox2d_examples_core" />
	<set-property name="user.agent" value="safari"/>	
	<entry-point class='org.flxbox2d.client.FlxBox2DExamplesStarter' />
	<set-configuration-property name="gdx.assetpath" value="../flixel-gdx-box2d-examples-android/assets" />
	
	<extend-configuration-property name="gdx.reflect.include" value="org.flxbox2d.examples" />
	<extend-configuration-property name="gdx.reflect.exclude" value="org.flxbox2d.examples.Box2DDemo" />
	<extend-configuration-property name="gdx.reflect.exclude" value="org.flxbox2d.examples.Cache" />
	<extend-configuration-property name="gdx.reflect.exclude" value="org.flxbox2d.examples.objects.Ghost" />
	<extend-configuration-property name="gdx.reflect.exclude" value="org.flxbox2d.examples.objects.RagDoll" />
</module>
